{
    "robotYarpPortPrefix": "/icubSim",
    "localYarpPortPrefix": "/yarpjs",
    "commonHostIPaddress": "localhost",
    "telemVizServer": {
        "host": "${this.commonHostIPaddress}",
        "port": 8081
    },
    "consoleServer": {
        "host": "${this.commonHostIPaddress}",
        "port": 3000
    },
    "openmctStaticServer": {
        "host": "${this.commonHostIPaddress}",
        "port": 8080
    },
    "portInConfig": {
        "icubtelemetry.IMUsens.legacyIMU": {
            "yarpName": "${this.robotYarpPortPrefix}/inertial",
            "localName": "${this.localYarpPortPrefix}/inertial:i",
            "portType": "bottle",
            "parser": {
                "type": "internal",
                "outputFormat": "fromId"
            },
            "sourceSync": "localTimer"
        },
        "icubtelemetry.jointState.leftLegJointState": {
            "yarpName": "${this.robotYarpPortPrefix}/left_leg/stateExt:o",
            "localName": "${this.localYarpPortPrefix}/left_leg/stateExt:o",
            "portType": "bottle",
            "parser": {
                "type": "internal",
                "outputFormat": "fromId"
            },
            "sourceSync": "localTimer"
        },
        "icubtelemetry.jointState.rightLegJointState": {
