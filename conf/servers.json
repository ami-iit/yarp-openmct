            "sourceSync": "localTimer"
        },
        "icubtelemetry.jointState.rightArmJointState": {
            "yarpName": "${this.robotYarpPortPrefix}/right_arm/stateExt:o",
            "localName": "${this.localYarpPortPrefix}/right_arm/stateExt:o",
            "portType": "bottle",
            "parser": {
                "type": "internal",
