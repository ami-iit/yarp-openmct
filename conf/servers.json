            "localName": "${this.localYarpPortPrefix}/right_leg/stateExt:o",
            "portType": "bottle",
            "parser": {
                "type": "internal",
                "outputFormat": "fromId"
            },
            "sourceSync": "localTimer"
        },
        "icubtelemetry.jointState.leftArmJointState": {
            "yarpName": "${this.robotYarpPortPrefix}/left_arm/stateExt:o",
            "localName": "${this.localYarpPortPrefix}/left_arm/stateExt:o",
            "portType": "bottle",
